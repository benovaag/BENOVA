<body class="page-case">
    <link rel="stylesheet" type="text/css" href="/css/criar-demanda.css">

{{>header}}
<section class="banner-demanda">
    <picture>
        <source media="(max-width: 600px)" srcset="/images/demandas/banner-mobile.jpg">
        <img src="/images/demandas/banner-desktop.jpg" alt="Banner desktop">
    </picture>
</section>

<section class="criar-demanda">
    <iframe class="clickup-embed clickup-dynamic-height"
    src="https://forms.clickup.com/36935390/f/1375py-1723/8VFEI59C8C01OORZHQ"
    onwheel="" width="100%" height="100%" style="background: transparent;border: none">
    </iframe>
    <!-- <script async src="https://app-cdn.clickup.com/js/forms-embed/v1.js"></script> -->
</section>

<script type="text/javascript">
window.addEventListener("DOMContentLoaded", ()=>{

  //Adiciona script do clickUp
  var iframeScript = document.createElement('script');
  iframeScript.src = 'https://app-cdn.clickup.com/assets/js/forms-embed/v1.js';
  document.querySelector(".criar-demanda").appendChild(iframeScript);

})
</script>

{{>footer}}
</body>
